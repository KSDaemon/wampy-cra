{
  "name": "wampy-cra",
  "version": "0.1.2",
  "description": "Wampy.js challenge response authentication (CRA) plugin",
  "author": "Konstantin Burkalev <KSDaemon@ya.ru> (http://ksdaemon.ru)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KSDaemon/wampy-cra/issues"
  },
  "homepage": "https://github.com/KSDaemon/wampy-cra",
  "main": "build/wampy-cra.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/KSDaemon/wampy-cra.git"
  },
  "dependencies": {
    "crypto-js": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.1",
    "grunt": "^1.5.2",
    "grunt-babel": "^8.0.0",
    "grunt-contrib-uglify": "^4.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "load-grunt-tasks": "^5.1.0",
    "mocha": "^7.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0"
  },
  "keywords": [
    "WAMP",
    "wampy",
    "CRA",
    "Challenge",
    "Response",
    "Authentication"
  ],
  "scripts": {
    "build": "grunt",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "NODE_ENV=test nyc ./node_modules/mocha/bin/mocha --exit  --require babel-core/register -R spec",
    "cover": "nyc report"
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "report-dir": "./coverage",
    "temp-dir": "./coverage",
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ]
  }
